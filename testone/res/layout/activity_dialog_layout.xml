<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <Button
        android:id="@+id/dialog_main_progress_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="进度条对话框" />

    <Button
        android:id="@+id/dialog_main_data_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="日期对话框" />

    <Button
        android:id="@+id/dialog_main_time_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="时间对话框" />

    <Button
        android:id="@+id/dialog_main_back_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="返回" />

    <Button
        android:id="@+id/dialog_main_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="自定义对话框" />

</LinearLayout>